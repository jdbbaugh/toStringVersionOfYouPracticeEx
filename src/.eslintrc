{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 1,
        "quotes": [1, "double"],
        "eqeqeq": 2,
        "no-trailing-spaces": ["error", { "skipBlankLines": true }]
    }
  }
